{
    "name": "poolmoojil",
    "public": true,
    "version": 2,
    "builds": [
        {
            "src": "now-entry.js",
            "use": "@now/node",
            "config": {
                "helpers": false,
                "includeFiles": ["dist/**/*.js"]
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "now-entry.js"
        }
    ]
}
